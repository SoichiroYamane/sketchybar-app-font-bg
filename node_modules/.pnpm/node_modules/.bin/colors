#!/bin/sh
basedir=$(dirname "$(echo "$0" | sed -e 's,\\,/,g')")

case `uname` in
    *CYGWIN*) basedir=`cygpath -w "$basedir"`;;
esac

if [ -z "$NODE_PATH" ]; then
  export NODE_PATH="/Users/yamanesouichirou/icon/sketchybar-app-font/node_modules/.pnpm/colors-cli@1.0.33/node_modules/colors-cli/bin/node_modules:/Users/yamanesouichirou/icon/sketchybar-app-font/node_modules/.pnpm/colors-cli@1.0.33/node_modules/colors-cli/node_modules:/Users/yamanesouichirou/icon/sketchybar-app-font/node_modules/.pnpm/colors-cli@1.0.33/node_modules:/Users/yamanesouichirou/icon/sketchybar-app-font/node_modules/.pnpm/node_modules"
else
  export NODE_PATH="/Users/yamanesouichirou/icon/sketchybar-app-font/node_modules/.pnpm/colors-cli@1.0.33/node_modules/colors-cli/bin/node_modules:/Users/yamanesouichirou/icon/sketchybar-app-font/node_modules/.pnpm/colors-cli@1.0.33/node_modules/colors-cli/node_modules:/Users/yamanesouichirou/icon/sketchybar-app-font/node_modules/.pnpm/colors-cli@1.0.33/node_modules:/Users/yamanesouichirou/icon/sketchybar-app-font/node_modules/.pnpm/node_modules:$NODE_PATH"
fi
if [ -x "$basedir/node" ]; then
  exec "$basedir/node"  "$basedir/../colors-cli/bin/colors" "$@"
else
  exec node  "$basedir/../colors-cli/bin/colors" "$@"
fi
