#!/bin/sh
basedir=$(dirname "$(echo "$0" | sed -e 's,\\,/,g')")

case `uname` in
    *CYGWIN*) basedir=`cygpath -w "$basedir"`;;
esac

if [ -z "$NODE_PATH" ]; then
  export NODE_PATH="/Users/yamanesouichirou/icon/sketchybar-app-font/node_modules/.pnpm/ttf2woff@3.0.0/node_modules/ttf2woff/node_modules:/Users/yamanesouichirou/icon/sketchybar-app-font/node_modules/.pnpm/ttf2woff@3.0.0/node_modules:/Users/yamanesouichirou/icon/sketchybar-app-font/node_modules/.pnpm/node_modules"
else
  export NODE_PATH="/Users/yamanesouichirou/icon/sketchybar-app-font/node_modules/.pnpm/ttf2woff@3.0.0/node_modules/ttf2woff/node_modules:/Users/yamanesouichirou/icon/sketchybar-app-font/node_modules/.pnpm/ttf2woff@3.0.0/node_modules:/Users/yamanesouichirou/icon/sketchybar-app-font/node_modules/.pnpm/node_modules:$NODE_PATH"
fi
if [ -x "$basedir/node" ]; then
  exec "$basedir/node"  "$basedir/../ttf2woff/ttf2woff.js" "$@"
else
  exec node  "$basedir/../ttf2woff/ttf2woff.js" "$@"
fi
