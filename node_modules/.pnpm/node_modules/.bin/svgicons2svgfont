#!/bin/sh
basedir=$(dirname "$(echo "$0" | sed -e 's,\\,/,g')")

case `uname` in
    *CYGWIN*) basedir=`cygpath -w "$basedir"`;;
esac

if [ -z "$NODE_PATH" ]; then
  export NODE_PATH="/Users/yamanesouichirou/icon/sketchybar-app-font/node_modules/.pnpm/svgicons2svgfont@12.0.0/node_modules/svgicons2svgfont/bin/node_modules:/Users/yamanesouichirou/icon/sketchybar-app-font/node_modules/.pnpm/svgicons2svgfont@12.0.0/node_modules/svgicons2svgfont/node_modules:/Users/yamanesouichirou/icon/sketchybar-app-font/node_modules/.pnpm/svgicons2svgfont@12.0.0/node_modules:/Users/yamanesouichirou/icon/sketchybar-app-font/node_modules/.pnpm/node_modules"
else
  export NODE_PATH="/Users/yamanesouichirou/icon/sketchybar-app-font/node_modules/.pnpm/svgicons2svgfont@12.0.0/node_modules/svgicons2svgfont/bin/node_modules:/Users/yamanesouichirou/icon/sketchybar-app-font/node_modules/.pnpm/svgicons2svgfont@12.0.0/node_modules/svgicons2svgfont/node_modules:/Users/yamanesouichirou/icon/sketchybar-app-font/node_modules/.pnpm/svgicons2svgfont@12.0.0/node_modules:/Users/yamanesouichirou/icon/sketchybar-app-font/node_modules/.pnpm/node_modules:$NODE_PATH"
fi
if [ -x "$basedir/node" ]; then
  exec "$basedir/node"  "$basedir/../svgicons2svgfont/bin/svgicons2svgfont.js" "$@"
else
  exec node  "$basedir/../svgicons2svgfont/bin/svgicons2svgfont.js" "$@"
fi
